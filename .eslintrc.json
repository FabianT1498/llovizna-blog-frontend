{
  "extends": [
    "cheesecakelabs/react",
    "plugin:import/recommended"
  ],
  "plugins": [
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "jsx-a11y/label-has-for": [
      2,
      {
        "required": {
          "every": [
            "nesting",
            "id"
          ]
        },
        "allowChildren": true
      }
    ]
  }
}
